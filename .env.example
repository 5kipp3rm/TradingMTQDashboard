# Environment variables for TradingMTQ
# Copy this file to .env and fill in your actual credentials

# MetaTrader 5 Credentials
MT5_LOGIN=your_account_number_here
MT5_PASSWORD=your_password_here
MT5_SERVER=your_broker_server_here

# Example:
# MT5_LOGIN=12345678
# MT5_PASSWORD=MySecurePassword123
# MT5_SERVER=MetaQuotes-Demo

# Phase 4+ LLM API Keys (uncomment when needed)
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Phase 5+ Database Configuration (uncomment when needed)
# DATABASE_URL=postgresql://user:password@localhost:5432/tradingmtq
# REDIS_URL=redis://localhost:6379

# Application Settings
# LOG_LEVEL=INFO
# ENVIRONMENT=development
